<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0)">Administración</a></li>
            <li class="breadcrumb-item active">Usuarios</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <div class="card card-body">
            <h4 class="card-title">Usuarios</h4>
            <h5 class="card-subtitle"> Formulario de Usuarios </h5>
            <div class="form-group">
                <fieldset disabled>
                    <label>Usuario <span class="help"> </span></label>
                    <input type="text" id="disabledTextInput" class="form-control" value="{{user.usuario}}">
                </fieldset>
            </div>

            <div class="form-group">
                <fieldset disabled>
                    <label>Nombre <span class="help"> </span></label>
                    <input type="text" id="disabledTextInput" class="form-control" value="{{user.nombre}}">
                </fieldset>
            </div>
            <div class="form-group">
                <fieldset disabled>
                    <label>Apellido <span class="help"> </span></label>
                    <input type="text" id="disabledTextInput" class="form-control" value="{{user.apellido}}">
                </fieldset>
            </div>
            <div class="form-group">
                <fieldset disabled>
                    <label for="example-email">Correo Electrónico </label>
                    <input type="email" id="disabledTextInput" name="example-email" class="form-control"
                        value="{{user.correo}}">
                </fieldset>
            </div>
            <div class="form-group">
                <fieldset disabled>
                    <label>Perfil <span class="help"> </span></label>
                    <input type="text" id="disabledTextInput" class="form-control" value="{{user.perfil}}">
                </fieldset>
            </div>
        </div>
    </div>
</div>